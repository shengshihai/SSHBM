    <script>
        function SuccessReName(jsonData, statusMsg) {
            Success(jsonData, statusMsg, this.url, succeedrename);
        }
        function succeedrename(jsonData) {
            alert(jsonData.Message);
            $("#resume_name .nameShow h1").attr("title", jsonData.Data.RESUME_NAME).text(jsonData.Data.RESUME_NAME).parent().removeClass("dn");
            $("#resume_name form").addClass("dn");
            $("#lastChangedTime span").text(jsonData.Data.SYNCVERSION.substring(0, jsonData.Data.SYNCVERSION.lastIndexOf("T")));
            $("#VERSION").val(jsonData.Data.VERSION);
        }
        function SuccessBaseinfo(jsonData, statusMsg) {
            Success(jsonData, statusMsg, this.url, succeedbaseinfo);
        }
        function succeedbaseinfo(jsonData) {
            jsonData.Data.EDUCATION = $("#box_topDegree ul").find("li[val='" + jsonData.Data.EDUCATION + "']").html();
            jsonData.Data.EXPERIENCE = $("#box_workyear ul").find("li[val='" + jsonData.Data.EXPERIENCE + "']").html();
            jsonData.Data.CURRENT_STATE = $("#box_currentState ul").find("li[val='" + jsonData.Data.CURRENT_STATE + "']").html();
            var strhtml = jsonData.Data.NAME + " | " + (jsonData.Data.SEX == "MAN" ? "男" : "女") + " | " + jsonData.Data.EDUCATION + " | " + jsonData.Data.EXPERIENCE + "<BR>";
            strhtml = strhtml + jsonData.Data.PHONE + " | " + jsonData.Data.EMAIL + "<BR>";
            strhtml = strhtml + jsonData.Data.CURRENT_STATE + "<BR>";
            $("#basicInfo .basicShow span").html(strhtml).parent().removeClass("dn");
            $("#basicInfo .basicEdit").addClass("dn");
            $("#basicInfo .c_edit").removeClass("dn");
        }
        function descaftersave(jsonData, statusMsg) {
            Success(jsonData, statusMsg, this.url, savedesc);
        }
        function savedesc(jsonData) {
            // var d = $("#descriptionForm #selfDescription").val();
            $("#selfDescription .c_edit").removeClass("dn");
            $("#selfDescription .descriptionShow").html(jsonData.Data.RESUME_DTL1).removeClass("dn");
            $("#selfDescription .descriptionEdit").addClass("dn");
        }
        function SaveWorkForm(jsonData, statusMsg) {
            Success(jsonData, statusMsg, this.url, savework);
        }
        function savework(jsonData) {
            // var d = $("#descriptionForm #selfDescription").val();
            $("#worksShow .c_add").removeClass("dn");
            $("#worksShow .workShow").html(jsonData.Data.RESUME_DTL1).removeClass("dn");
            $("#worksShow .workEdit").addClass("dn");
        }
    </script>
